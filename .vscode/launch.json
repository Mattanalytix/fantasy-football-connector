{
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            }
        },
        {
            "name": "Cloud Run: Run/Debug Locally",
            "type": "cloudcode.cloudrun",
            "request": "launch",
            "build": {
                "docker": {
                    "path": "Dockerfile"
                }
            },
            "image": "fantasy-football-connector",
            "service": {
                "name": "fantasy-football-connector",
                "containerPort": 8080,
                "env": [
                    {
                        "name": "LOCAL_ENV",
                        "value": "true"
                    },
                    {
                        "name": "PROJECT_ID",
                        "value": "raw-nonprod-service-k41"
                    },
                    {
                        "name": "DATASET_ID",
                        "value": "dev_mhenderson"
                    },
                    {
                        "name": "BUCKET_ID",
                        "value": "fantasy-premier-league-etl-nonprod"
                    }
                ],
                "resources": {
                    "limits": {
                        "memory": "256Mi"
                    }
                }
            },
            "target": {
                "minikube": {}
            },
            "watch": true,
            "debug": {
                "sourceFileMap": {
                    "${workspaceFolder}": "/app"
                }
            }
        }
    ]
}